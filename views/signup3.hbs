<h2>Select images</h2>

    <table class="table mt-5" id="productsTable">
        <tbody>
            {{#each category}}
            <tr>
                <th>
                <td>{{this.category}}</td>
                </th>
                <th>
                <td><img style="width: 50px; height:50px" src="/images/{{this._id}}.jpg" alt=""></td>
                </th>
                <th>
                <td>
                    {{this.alpha}}
                </td>
                </th>
            </tr>
            {{/each}}
        </tbody>
    </table>

<form action="endRegistration" method="POST">
    <label for="">Password</label>
    <input type="password" class="form-control" name="password" placeholder="Enter Password" required>
    <button type="submit" class="btn btn-primary float-right">Done</button>
</form>